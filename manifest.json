{
	"manifest_version": 2,
	
	"name": "Facebook CLI",
	"version": "1.0",

	"permissions": [
		"tabs", "http://*/*"
	],

	"content_scripts": [
		{
			"matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"],
			"js": ["jquery.js", "content.js"]
		}
	],

	"browser_action": {
		"default_popup": "popup.html"
	}
}
